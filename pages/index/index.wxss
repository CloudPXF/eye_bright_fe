/**index.wxss**/
@import "../../wxParse/wxParse.wxss";
/* 首页 */
.index-page {
  
}
.index-main {
  padding-bottom: 100rpx;
}
swiper {
  height: 360rpx;
}
swiper-item image {
  width: 100%;
  height: 100%;
}
.index-search {
  padding: 20rpx 36rpx;
}
.index-search-box {
  height: 56rpx;
  border-radius: 10rpx; 
  background: #f7f7f7;
  display: flex;
  align-items: center;
}
.index-search-box image {
  width: 26rpx;
  height: 26rpx;
  margin-left: auto;
  margin-right: 10rpx;
}
.index-search-box input {
  font-size: 26rpx;
  width: 270rpx;
  margin-right: auto;
}
.index-search-line{
  border-bottom: 1rpx solid #f4f4f4;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.index-search-word {
  display: flex;
  height: 60rpx;
  align-items: center;
}
.index-search-word-title {
  font-size: 24rpx;
  color: #999;
  width: 100rpx;
}
.index-search-word-list {
  height: 100%;
  width: 84%;
  white-space: nowrap;
}
.index-search-word-item {
  display: inline-block;
  background: #f4f4f4;
  height: 56rpx;
  border-radius: 28rpx;
  font-size: 22rpx;
  color: #999;
  line-height: 56rpx;
  padding: 0 20rpx;
  margin-right: 16rpx;
}
.index-nav {
  border-top: 12rpx solid #f4f4f4;
  border-bottom: 12rpx solid #f4f4f4;
  display: flex;
  padding: 40rpx 0;
}
.index-nav-item {
  width: 25%;
  text-align: center;
}
.index-nav-item-img {

}
.index-nav-item-img image {
  width: 100rpx;
  height: 100rpx;
}
.index-nav-item-text {
  font-size: 22rpx;
  color: #333;
}

/* 问答 */
.request-main {
  padding-bottom: 100rpx;
}
.request-search {
  display: flex;
  align-items: center;
  height: 110rpx;
  background: #0064A0;
  width: 100%;
}
.request-search-box {
  display: flex;
  align-items: center;
  height: 56rpx;
  width: 520rpx;
  background: #fff;
  margin-left: 37rpx;
  margin-right: 27rpx;
  border-radius: 10rpx;
}
.request-search-box image {
  width: 26rpx;
  height: 26rpx;
  margin-left: auto;
  margin-right: 14rpx;
}
.request-search-box input {
  font-size: 26rpx;
  width: 220rpx;
  margin-right: auto;
}
.request-search-ask {
  width: 130rpx;
  height: 56rpx;
  line-height: 56rpx;
  border: 2rpx solid #fff;
  border-radius: 10rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}
.request-search-word {
  background: #fff;
  border-bottom: 12rpx solid #f4f4f4;
  padding-left: 38rpx;
  padding-top: 8rpx; 
}
.request-show-more {
  font-size: 24rpx;
  color: #333;
  text-align: center;
  height: 54rpx;
  line-height: 54rpx;
  position: relative;
  right: 38rpx;
}
.request-search-word-item {
  border: 2rpx solid #f4f4f4;
  font-size: 26rpx;
  color: #333;
  display: inline-block;
  padding: 17rpx 20rpx;
  border-radius: 10rpx;
  margin-right: 32rpx;
  margin-top: 34rpx;
}
.request-search-word-item-on {
  background: #0064A0;
  color: #fff;
}
.request-bottom {
  text-align: center;
  font-size: 22rpx;
   color: #999;
   background: #f4f4f4;
   height: 40rpx;
   line-height: 40rpx;
}

/* 我的 */
.my-main {
  height: 100vh;
  background: #f4f4f4;
}
.my-message {
  height: 200rpx;
  display: flex;
  align-items: center;
  background: #0064A0;
}
.my-message-avatar {
  margin-left: 36rpx;
}
.my-message-avatar image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}
.my-message-name {
  font-size: 32rpx;
  color: #fff;
  margin-left: 39rpx;
  font-weight: bold;
}
.my-message-to {
  margin-left: auto;
  margin-right: 36rpx;
}
.my-message-to image {
  width: 16rpx;
  height: 26rpx;
}
.my-func-list {
  margin-top: 24rpx;
}
.my-func-item {
  display: flex;
  height: 125rpx;
  align-items: center;
  background: #fff;
  border-bottom: 2rpx solid #f4f4f4;
}
.my-func-item-img {
  width: 46rpx;
  height: 46rpx;
  margin-left: 36rpx;
  margin-right: 20rpx;
}
.my-func-item-img image {
  width: 46rpx;
  height: 46rpx;
}
.my-func-item-text {
  font-size: 30rpx;
  color: #333;
}
.my-func-item-to {
  margin-left: auto;
  margin-right: 36rpx;
}
.my-func-item-to image {
  width: 16rpx;
  height: 26rpx;
}
.my-feedback-btn {
  width:90%;
  height: 84rpx;
  font-size: 32rpx;
  color: #fff;
  display: flex;
  align-items: center;
  background: #0064A0;
  border-radius: 10rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: 100rpx;
}
.my-feedback-btn image {
  width: 30rpx;
  height: 30rpx;
  margin-left: auto;
  margin-right: 19rpx;
} 

/* tabbar */
.tabbar {
  width: 100%;
  height: 100rpx;
  display: flex;
  position: fixed;
  z-index: 99;
  bottom: 0;
  box-shadow:0px 0px 14px 0px rgba(0, 0, 0, 0.1);
  background: #fff;
}
.tabbar-item {
  width: 25%;
  height: 100%;
}
.tabbar-item-img {
  text-align: center;
  padding-top: 10rpx;
}
.tabbar-item-img image {
  width: 46rpx;
  height: 46rpx;
}
.tabbar-item-text {
  text-align: center;
  font-size: 22rpx;
  color: #333;
}
.tabbar-item-text-on{
  color: #0064A0;
}

/* 授权 */
.container {
  position: fixed;
  top: 0;
  height: 100vh;
  width: 100vw;
  background: #fff;
  z-index: 101;
}
.userinfo {
  margin-top: 40rpx;
}
.userinfo button {
  width: 50%;
  margin-left: auto;
  margin-right: auto;
  background: #0064A0;
  color: #fff;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
  text-align: center;
}

/* 预约 */
.shop-main {
  background: #f6f6f6;
  min-height: 100vh;
  padding-bottom: 100rpx;
}
.appoint-search {
  padding: 22rpx;
  background: #0064A0;
}
.appoint-search-box {
  display: flex;
  align-items: center;
  height: 56rpx;
  width: 520rpx;
  margin-left: auto;
  margin-right: auto;
  border-radius: 10rpx;
  background: #fff;
}
.appoint-search-box image {
  width: 26rpx;
  height: 26rpx;
  margin-left: auto;
  margin-right: 14rpx;
}
.appoint-search-box input {
  width: 160rpx;
  font-size: 26rpx;
  color: #333;
  margin-right: auto;
}
.appoint-calendar {
  padding: 15rpx 0;
  background: #fff;
}
.appoint-calendar-title {
  padding: 25rpx 0;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}
.appoint-calendar-week {
  display: flex;
  height: 66rpx;
  background: #f6f6f6;
  margin-bottom:10rpx;
}
.appoint-calendar-date {
  color: #999;
  width: 66rpx;
  height: 66rpx;
  font-size: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 10rpx;
  text-align: center;
  line-height: 66rpx;
  display: inline-block;
  border-radius: 50%;
}
.appoint-calendar-date-on {
  background: #0064A0;
  color: #fff;
}
.appoint-calendar-date-list {
  padding-bottom: 10rpx;
}
.appoint-shop {
  margin-top: 12rpx;
  background: #fff;
}
.appoint-shop-title {
  padding-top: 40rpx;
  display: flex;
  align-items: center;
  padding-bottom: 10rpx;
}
.appoint-shop-title-before {
  height: 32rpx;
  width: 14rpx;
  background: #0064A0;
  border-radius: 5rpx;
  margin-left: 36rpx;
  margin-right: 20rpx;
}
.appoint-shop-title-text {
  font-size: 24rpx;
  color: #333;
}
.appoint-shop-title-time {
  font-size: 24rpx;
  color: #ed5656;
  margin-left: 31rpx;
}
.appoint-shop-list {

}
.appoint-shop-item {
  border-bottom: 2rpx solid #f4f4f4;
  padding-top: 50rpx;
  padding-bottom: 38rpx;
  padding-left: 35rpx;
  padding-right: 35rpx;
}
.appoint-shop-item-name {
  font-size: 30rpx;
  color: #333;
}
.appoint-shop-item-location {
  font-size: 26rpx;
  color: #666;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
}
.appoint-shop-item-time {
  font-size: 24rpx;
  color: #0064A0;
}